{
    "branches": [
        "main"
    ],
    "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        "@semantic-release/github"
    ],
    "github": {
        "release": true,
        "assets": [
            "build/**/*.tgz"
        ],
        "labels": [
            "release",
            "semver:major",
            "semver:minor",
            "semver:patch"
        ],
        "changelog": true
    }
}